{
  "name": "server",
  "version": "1.0.0",
  "description": "Node.js setup with Express and TypeScript",
  "main": "index.ts",
  "scripts": {
    "start": "node build/index.js",
    "build": "rimraf dist && tsc",
    "serve": "ts-node-dev --respawn --transpile-only -T -P ./tsconfig.json ./index.ts",
    "generate": "npx prisma generate && npx prisma db pull"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@twilio-labs/plugin-rtc": "^0.8.2",
    "@twilio/conversations": "^1.2.3",
    "@twilio/video-processors": "^1.0.1",
    "@twilio/video-room-monitor": "^1.0.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase": "^9.8.1",
    "firebase-admin": "^10.2.0",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "nodemailer": "^6.7.7",
    "rimraf": "^3.0.2",
    "stripe": "^9.12.0",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "twilio": "^3.77.1",
    "twilio-video": "^2.21.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
     "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.3",
    "@types/cors": "^2.8.12",
    "@types/nodemailer": "^6.4.5",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.19",
    "prisma": "^3.15.2"
  }
}
